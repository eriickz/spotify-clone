    "name": "spotify-clone",
    "slug": "spotify-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "spotify-clone",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff",
      "dark": {
        "backgroundColor": "#0D0C0C"
      }
